{
  "name": "kinology-backend",
  "version": "1.0.0",
  "description": "backend for kinology - a movie recommendation app",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_NEV=development nodemon index.js",
    "test": "cross-env NODE_ENV=test node --test",
    "lint": "eslint .",
    "eslint": "eslint --fix",
    "start:test": "cross-env NODE_ENV=test node index.js",
    "build:ui": "rm -rf dist && cd ../kinology_frontend && npm run build && cp -r dist ../kinology_backend",
    "deploy:full": "npm run build:ui && cd .. && git add . && git commit -m uibuild && git push "
  },
  "author": "timmchi",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.629.0",
    "@aws-sdk/s3-request-presigner": "^3.575.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.3",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "sharp": "^0.33.3",
    "valibot": "^0.31.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin-js": "^1.8.0",
    "@types/node": "^20.14.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.2.0",
    "nock": "^13.5.4",
    "nodemon": "^3.1.0",
    "supertest": "^7.0.0"
  }
}
